{
  "schemaVersion": 1,
  "id": "wynnlimbo",
  "version": "${version}",
  "name": "WynnLimbo",
  "description": "WynnLimbo makes waiting in queue more fun by providing you with a world to explore and parkour in!",
  "authors": ["_GoldenShadow"],
  "contact": {
    "sources": "https://github.com/GoldenShad0w/WynnLimbo",
    "issues": "https://github.com/GoldenShad0w/WynnLimbo/issues"
  },

  "license": "GPL-3.0",
  "icon": "assets/wynnlimbo/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "goldenshadow.wynnlimbo.client.WynnlimboClient"
    ],
    "main": [
      "goldenshadow.wynnlimbo.Wynnlimbo"
    ]
  },
  "mixins": [
    "wynnlimbo.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
